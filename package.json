{
  "name": "pg6301-innlevering-godvik",
  "version": "1.0.0",
  "description": "Applikasjonen skal vise at dere behersker:",
  "main": "index.js",
  "scripts": {
    "start": "cd server && npm start",
    "dev": "concurrently npm:dev:server npm:dev:client",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm install --include=dev && npm run build",
    "build:server": "cd server && npm install",
    "format": "prettier --write **/*.\"{js,jsx}\"",
    "format:check": "prettier --check **/*.\"{js,jsx}\"",
    "test": "jest --coverage",
    "verify": "npm run format:check && npm run test",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristiania-pg6301-2022/pg6301-innlevering-godvik.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-godvik/issues"
  },
  "homepage": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-godvik#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/supertest": "^2.0.11",
    "babel-jest": "^27.5.1",
    "concurrently": "^7.0.0",
    "express": "^4.17.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "pretty": "^2.0.0",
    "supertest": "^6.2.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.{jsx,js}",
      "!**/node_modules/**",
      "!coverage/**",
      "!**/dist/**",
      "index.jsx",
      "!**/questions.js",
      "!babel.config.js",
      "!**/server.js"
    ]
  },
  "engines": {
    "node": "16.13.2",
    "npm": "8.5.0"
  }
}
